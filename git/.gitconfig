[user]
	name = Shiv Adhiappan
	email = shivaram1190@gmail.com
[push]
	default = current
[alias]
        cl = clone
        ci = commit
        co = checkout
        br = for-each-ref --count=30 --sort=-committerdate refs/heads/ --format='%(refname:short)'
        r = reset
        sl = stash list
        sa = stash apply
        ss = stash save
        mg = mergetool -y -t opendiff
        lp = log --pretty=oneline --abbrev-commit
        cp = cherry-pick
[merge]
[diff]
[difftool]
	prompt = false
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[credential]
	helper = osxkeychain
[core]
	excludesfile = /Users/shivadhiappan/.gitignore_global
	editor = /usr/local/bin/vim
[tag]
	sort = version:refname
